{
  "name": "clippy-cloud-api",
  "version": "0.1.0",
  "main": "src/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "watch": "tsc -w",
    "dev": "cross-env NODE_ENV=development nodemon dist/server.js",
